<block wx:if="{{$taroCompReady}}">
    <view>
        <view class="index">
            <view class="icon">
                <image src="../../assets/images/shijiashi.jpg"></image>
            </view>
            <view class="introduce">
                <view class="title"><text>拾加拾教育：学商测试</text>
                </view>
                <view class="content"><text>测试内容：考察学生各方面能力</text>
                </view>
                <view class="content"><text>测试时间：不限</text>
                </view>
                <view class="content"><text>提交条件：题目全部完成后方可提交</text>
                </view>
                <view class="content"><text>测试形式：自主测试</text>
                </view>
            </view>
        </view>
        <view class="button">
            <button class="btn" bindtap="goTest" data-e-tap-so="this">开始测试</button>
            <button class="btn" bindtap="goResult" data-e-tap-so="this">查看结果</button>
        </view>
        <at-modal compid="{{$compid__192}}">
            <at-modal-content>
                <view class="tishi-img">
                    <image src="../../assets/images/shijiashi.jpg"></image>
                </view>
                <view class="quanxian-content">学商测试申请获得以下权限：</view>
                <view class="quanxian">获得你的公开信息(昵称、头像等)</view>
            </at-modal-content>
            <at-modal-action>
                <button class="btn" open-type="getUserInfo" bindgetuserinfo="getUserInfo">微信授权</button>
            </at-modal-action>
        </at-modal>
        <at-curtain compid="{{$compid__193}}">
            <view class="tishi">
                <at-form compid="{{$compid__194}}">
                    <at-input compid="{{$compid__195}}"></at-input>
                    <at-input compid="{{$compid__196}}"></at-input>
                    <at-input compid="{{$compid__197}}"></at-input>
                    <at-input compid="{{$compid__198}}"></at-input>
                    <view class="btn-sub">
                        <at-button compid="{{$compid__199}}">保存</at-button>
                    </view>
                </at-form>
            </view>
        </at-curtain>
    </view>
</block>