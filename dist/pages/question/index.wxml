<block wx:if="{{$taroCompReady}}">
    <view class="">
        <at-progress compid="{{$compid__28}}"></at-progress>
        <view class="question">
            <view class="item-box">
                <scroll-view class="scrollview" scroll-x="{{true}}" scroll-with-animation="{{true}}" scroll-left="{{scrollLeft}}" lower-threshold="{{Threshold}}" bindscroll="onScroll">
                    <view class="{{item.$loopState__temp3}}" bindtap="setCurrentQuestion" wx:for="{{loopArray7}}" wx:for-item="item" wx:for-index="_anonIdx2" data-e-tap-so="this" data-e-tap-a-a="{{item.$original - 1}}">{{item.$original}}</view>
                </scroll-view>
            </view>
            <view class="question-box">
                <view class="question-title">{{nowIndex + 1 + '【' + currentQuestionPartName + '】' + currentQuestion}}</view>
                <view class="answer-box">
                    <view class="{{chooesAnswer.value == item.value ? 'answer-item checked' : 'answer-item'}}" wx:key="index" bindtap="chooesAnswer" wx:for="{{currentAnswerList}}" wx:for-item="item" wx:for-index="index" data-e-tap-so="this" data-e-tap-a-a="{{item.value}}" data-e-tap-a-b="{{item.key}}">{{AnwserKey[item.value]}}.{{item.label}}</view>
                </view>
            </view>
            <view>
                <block wx:if="{{buttonShow}}">
                    <at-button compid="{{$compid__29}}">提交</at-button>
                </block>
            </view>
        </view>
    </view>
</block>